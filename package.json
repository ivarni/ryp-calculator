{
  "name": "ryp-calculator",
  "version": "1.1.2",
  "description": "Redux store, actions and reducers for a RYP calculator",
  "files": [
    "lib/"
  ],
  "scripts": {
    "dist": "babel -d lib/. --ignore test/ src/",
    "dist:watch": "babel -d lib/. --ignore test/ src/ --watch",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "prepublish": "npm run dist",
    "test": "tape -r babel-register test/**/*.js | faucet",
    "test:watch": "tape-watch -r babel-register test/**/*.js | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivarni/ryp-calculator.git"
  },
  "author": "Ivar Nilsen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ivarni/ryp-calculator/issues"
  },
  "homepage": "https://github.com/ivarni/ryp-calculator#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.1.2",
    "faucet": "0.0.1",
    "tape": "^4.6.0",
    "tape-watch": "^2.2.3"
  },
  "dependencies": {
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  }
}
